{
  "compilerOptions": {
    "sourceMap": true,
    "lib": [
      "es5",
      "es6",
      "dom",
      "es2015",
      "esnext.asynciterable"
    ],
    "preserveConstEnums": true,
    "strictNullChecks": true,
    "target": "es5",
    "outDir": ".build",
    "moduleResolution": "node",
    "rootDir": "./",
    "downlevelIteration": true
  },
  "include": [
    "scripts/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
